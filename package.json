{
  "name": "moment-stack-backend",
  "version": "1.0.0",
  "description": "Backend API for Moment Stack",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts",
    "clean": "rimraf dist",
    "migrate": "ts-node -e \"import('./dist/config/migrations').then(m => m.MigrationRunner.runMigrations())\"",
    "migrate:dev": "ts-node -e \"import('./src/config/migrations').then(m => m.MigrationRunner.runMigrations())\"",
    "reset-db": "ts-node -e \"import('./dist/config/migrations').then(m => m.MigrationRunner.resetDatabase())\"",
    "reset-db:dev": "ts-node -e \"import('./src/config/migrations').then(m => m.MigrationRunner.resetDatabase())\""
  },
  "keywords": ["node", "express", "typescript", "api", "moment-stack"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5"
  }
}